    <div id="ads_right">
        <script type="text/javascript">
			if(quoteBoxSelectedSymbol!="VNTH")
            switch (Math.floor(Math.random() * 3) + 1) {
                case 1: makeAd('14', '300x250,300x1050', is_dartSite, is_dartZone, '', '');
                    break;
                case 2: makeAd('14', '300x250,300x1050', is_dartSite, is_dartZone, '', '');
                    break;
                case 3: makeAd('14', '300x250,300x1050', is_dartSite, is_dartZone, '', '');
                    break;
            }
        </script>

