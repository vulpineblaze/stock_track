    <script type="text/javascript">
var quoteBoxSelectedSymbol="AIMCO.BO";

var quoteBoxSymbolArray = new Array(1);
quoteBoxSymbolArray[0] = "AIMCO.BO";

var quoteBoxNumSymbols = 1;

var jsUserAgent = "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.11 (KHTML, like Gecko) Chrome/23.0.1271.64 Safari/537.11";

var qbUseNewNav = true;
</script>

<script type="text/javascript" language="JavaScript"><!--inline script should be added to .inc files if we cannot added to if browser caching is a concern-->
document.domain = 'nasdaq.com';

// URL variables
	var quoteURL = 'http://www.nasdaq.com/';
	var wwwURL = 'http://www.nasdaq.com/';
	var earningsURL = 'http://www.nasdaq.com/';
	var screeningURL = 'http://www.nasdaq.com/';
	var bLookup = false;
	var selected = quoteBoxSelectedSymbol;

	if(typeof qbUseNewNav == "undefined")
		qbUseNewNav = false;

if(quoteBoxNumSymbols>13)
{
	addStyle("dynamic","#quotesnav_symbols_l {display:block;}");//
	addStyle("dynamic","#quotesnav_symbols_l_border {display:block;}");//
}
else
{
	addStyle("dynamic","#quotesnav_symbols_l {display:none;}");//
	addStyle("dynamic","#quotesnav_symbols_l_border {display:none;}");//
}

addStyle("dynamic","div.qbreadcrumb {left: 0px !important;height:0px;left:0px;top:0px;position:relative;width:800pxfloat:left;}");
addStyle("dynamic","#qtopAd-graybg{float:left;font-size:11px;height:33px;padding:17px 0 0 5px;width:661px;position:relative;}");//
addStyle("dynamic","#qtopAd-graybg-inside, #qtopAd-sponsored{float:left;}");//

function loginForAlerts()
{
	open_new_window("http://community.nasdaq.com/login.aspx?turl="+encodeURIComponent("http://www.nasdaq.com/alerts/alertmanager.aspx?symbol="+quoteBoxSelectedSymbol),1020,980,false,true,false);
}
</script>

