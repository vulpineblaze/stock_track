//refresh the page, track the event in omniture
function refresh()
{
	triggerAsyncOmnitureClickEvent('quotes:refresh:summaryquote');
	window.location.reload();
}

function makeAd(adTile, adSize, adSection, adSubsection, adKeyWords, adDcopt){
	
	if(!allowAdCall(adTile, adSize, adSection, adSubsection, adKeyWords, adDcopt))
		return;

	if (adSubsection == "newshub") //any page that has "newshub" as the zone, spit out a 728x90 google ad, but use a standard makead call for 185x35
	{
		if(adSize=='185x35')//this is a 185x35 makead call on a newshub page, display a standard ad
		{
			makeAd('2','185x35','home','ROSToolbar','','');
			return;
		}
		else//this is a 728x90 makead call on a newshub page, display a special google ad
		{
			document.write("<div id='div-gpt-ad-1354568488163-3' style='width:728px; height:90px;'></div>");
			googletag.cmd.push(function() { googletag.display('div-gpt-ad-1354568488163-3'); });
			return;
		}
	}
		var axel = Math.random() + "";//random number in the range 0 - 1
		var ord = axel * 1000000000000000000;//multiply by a large number to get rid of decimals
		var adTileIndex = adTile.indexOf('p')//see if 'p' exists in the adTile variable, if it's -1 p doesn't exist in adTile
		var kwString ='';
		var dcopt = ''
		if (adDcopt == 'y'){
			dcopt = 'dcopt=ist';
		}
		
			var adTicker;
			adTicker = getUserSelectedFromURL()//grabs symbol from the url
			if(adTicker!=false)//found a symbol
			{
				kwString += "tkr="+adTicker.toLowerCase()+";";//add the symbol in lowercase to the keyword string preceded by "tkr="
				if(adTicker.toLowerCase()=="ndaq" || adTicker.toLowerCase()=="qqq")
					kwString += "NDQ=yes;";
				else
					kwString += "NDQ=no;";
			}
			else//no symbol found
			{
				kwString += "NDQ=no;";
			}
		
		//obselete code for
		if(/-cm\d+$/i.test(document.location.href))
		{
			if(typeof propVar != "undefined")
				kwString += "key="+propVar.toLowerCase();
			if(typeof allSubCategories != "undefined")
				kwString += ",key=" + allSubCategories.toLowerCase().replace(/;/g, ",key=");
			kwString += ";";
		}

			
		if ( adTileIndex>= 0){// 'p' exists in adTile
			var adPTile = adTile.substring(1, adTile.length)//strip out everything except for the first character 'p'
			if(adKeyWords!=""){parseKeyWords(adKeyWords)}
			document.write ('<scr' + 'ipt src="http://ad.doubleclick.net/adj/' + adSection + '.nasdaq.com/' + adSubsection + ';tile=' + adPTile + ';' + dcopt + ';abr=!webtv\;' + kwString + segQS + 'key=value\;sz=' +adSize + ';ord={' + dartPtileRan +'}?"></scr' + 'ipt>');
		} else {
			if(adKeyWords!=""){parseKeyWords(adKeyWords)}
			document.write ('<scr' + 'ipt src="http://ad.doubleclick.net/adj/' + adSection + '.nasdaq.com/' + adSubsection + ';tile=' + adTile + ';' + dcopt + ';abr=!webtv\;' + kwString + segQS + 'key=value\;sz=' +adSize + ';ord={' + dartPtileRan +'}?"></scr' + 'ipt>');
		}
		function parseKeyWords(){
			var kwArray = adKeyWords.split('|');
			var kwaCount;
			var kwaLength = kwArray.length;
			for (kwaCount = 0; kwaCount < kwaLength; kwaCount++){
				kwString += 'kw=' + kwArray[kwaCount] + '\;';
			}
			return kwString;
		}
	}
        //-->
    </script>

