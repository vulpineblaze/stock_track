    <style type="text/css">
        #quotepageheader #quotetools {
            padding: 5px;
            width: 100%;
        }
        .iq_buttons table td {
            padding-right: 10px;
        }
        .iq_buttons td a {
            font-size: 11px;
            font-weight: bold;
            display: block;
            float: left;
            margin-left: 5px;
        }
        table#quotes_content_left_ETFDataList {
    	    border: 1px solid #cbeef4;
    	    color: #404040;
    	    font-family: Arial, Helvetica, sans-serif;
    	    font-size: 12px;
    	    width: 100%;  
	    }
	    #quotes_content_left_ETFDataList th.nextgen {
            color: #404040;
	        border: none;
	        background: #cbeef4;
	        padding: 8px;
		    line-height: normal;
	        text-align: left; 
	        font-weight: bold;
	    }
	    #quotes_content_left_ETFDataList td.nextgen {
	        border-style: solid none;
	        border-bottom: 1px solid #cbeef4;
	        padding: 8px;  
	        vertical-align: top;
	        line-height: normal
        }
        #quotes_content_left_ETFDataList tr:hover {
	        background: #e3f6f9;
	        cursor: default;
        }
    </style>
