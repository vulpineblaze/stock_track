    <!-- START page bottom script -->
    <script language="javascript" type="text/javascript">
        var sleeptime = 7;var today = new Date();var timerid = null;var xmlhttp = null;
        if(today.getDay() == 6 || today.getDay() == 0 || today.getHours() < 9 || today.getHours() > 17)
        {sleeptime = 9; /*Sleep time during non business hours*/}
        else
        {sleeptime = 7; /*Sleep time during business hours */}
        function setCookie(c_name,value,expiredays){var exdate=new Date();exdate.setDate(exdate.getDate()+expiredays); document.cookie=c_name+ "=" +escape(value)+((expiredays==null) ? "" : ";expires="+exdate.toGMTString());}
        function getCookie(c_name){ if (document.cookie.length>0){c_start=document.cookie.indexOf(c_name + "=");if (c_start!=-1){c_start=c_start + c_name.length+1;c_end=document.cookie.indexOf(";",c_start);if (c_end==-1) c_end=document.cookie.length;return unescape(document.cookie.substring(c_start,c_end));}}return "";}
        function getQuoteData() {

            var Streaming = getCookie('streamingdata');
            if(Streaming == "On")
            {
                document.getElementById('DynamicSwitch').innerHTML = "Off";
           	    setCookie('streamingdata','On',365);
                getData();
           	
            }
            else
            {
              document.getElementById('DynamicSwitch').innerHTML = "On";
              setCookie('streamingdata','Off',365);
            }
        }
        function getData()
        {
            if(window.XMLHttpRequest)
            {
                xmlhttp = new XMLHttpRequest();
            }
            else if(window.ActiveXObject)
            {
                xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
            if(xmlhttp == null)
            {
                alert("Browser does not support dynamic data");
            }
            else
            {
                var Url = "../quotedll/quote.dll?page=InfoQuotes&mode=stock&symbo=BHAGCHEM.BO&selected=BHAGCHEM.BO&"+Math.random();
                xmlhttp.onreadystatechange = UpdateQuoteData;
                xmlhttp.open("GET",Url,true);
                xmlhttp.send(null);
                timerid = self.setTimeout("getQuoteData()",sleeptime*1000);
            }
        }
       function UpdateQuoteData()
        {
            if(xmlhttp.readyState == 4 || xmlhttp.readyState == "complete")
            {
                if(xmlhttp.responseText != "")    
                {
                    var symboldata = new Array();
                    var changesign = "";
    			    symboldata = xmlhttp.responseText.split("<*>");
    			    for (var i = 1; i < symboldata.length; i ++)
    			    {
    			        var symbolvalues = new Array();
    			        symbolvalues = symboldata[i].split("|");
    			        // Update LastSale Value
    			        document.getElementById("qwidget_lastsale").innerHTML = symbolvalues[5];
    			       // document.getElementById(symbolvalues[0]+"_LastSale1").innerHTML = symbolvalues[5];
    			       // document.getElementById(symbolvalues[0]+"_LastSale2").innerHTML = symbolvalues[5];
    			        // Update the Volume
    			        document.getElementById(symbolvalues[0]+"_Volume").innerHTML = symbolvalues[6];
    			        // Market Status
    			        document.getElementById("qwidget_markettime").innerHTML = symbolvalues[26];

    			      //  document.getElementById(symbolvalues[0]+"_MarketStatus").innerHTML = symbolvalues[26];
    			        // Percentage Value
    			      //  document.getElementById(symbolvalues[0]+"_Per1").innerHTML = symbolvalues[23];
    			      //  document.getElementById(symbolvalues[0]+"_Per2").innerHTML = symbolvalues[23];
    			        // Netchange Value
    			        document.getElementById("qwidget_netchange").innerHTML = symbolvalues[22];
    			        document.getElementById("qwidget_percent").innerHTML = symbolvalues[23];

    			      //  document.getElementById(symbolvalues[0]+"_Chg1").innerHTML = symbolvalues[22];
    			      //  document.getElementById(symbolvalues[0]+"_Chg2").innerHTML = symbolvalues[22];
    			        // Image Value
    			        if(symbolvalues[24]=='up') {

    			            //document.getElementById("qwidget-arrow-image").src = "http://content.nasdaq.com/images/greenArrowSmall.gif";
    			           // document.getElementById(symbolvalues[0]+"_Img1").src = "http://content.nasdaq.com/images/greenArrowSmall.gif";
    			            // document.getElementById(symbolvalues[0]+"_Img2").src = "http://content.nasdaq.com/images/greenArrowSmall1.gif";
    			            document.getElementById("qwidget_netchange").setAttribute("class", "qwidget-cents qwidget-Green");
    			            document.getElementById("qwidget_percent").setAttribute("class", "qwidget-percent qwidget-Green");
    			            changesign = "+";
    			       
    			        }
    			        else if(symbolvalues[24]=='down') {

    			            //document.getElementById("qwidget-arrow-image").src = "http://content.nasdaq.com/images/redArrowSmall.gif";
    			           // document.getElementById(symbolvalues[0]+"_Img1").src = "http://content.nasdaq.com/images/redArrowSmall.gif";
    			            // document.getElementById(symbolvalues[0]+"_Img2").src = "http://content.nasdaq.com/images/redArrowSmall1.gif";
    			            document.getElementById("qwidget_netchange").setAttribute("class", "qwidget-cents qwidget-Red");
    			            document.getElementById("qwidget_percent").setAttribute("class", "qwidget-percent qwidget-Red");
    			            changesign = "-";
    			        }
    			        else if(symbolvalues[24]=='unch') {
    			            //document.getElementById("qwidget-arrow-image").src = "http://content.nasdaq.com/images/1point.gif";
    			            //document.getElementById(symbolvalues[0]+"_Img1").src = "http://content.nasdaq.com/images/1point.gif";
    			            //document.getElementById(symbolvalues[0]+"_Img2").src = "http://content.nasdaq.com/images/1point.gif";
    			            document.getElementById("qwidget_netchange").setAttribute("class", "");
    			            document.getElementById("qwidget_percent").setAttribute("class", "");
    			        }
    			        document.title = symbolvalues[0] + " : $" + symbolvalues[46].toString() + changesign + symbolvalues[22] + " (" + symbolvalues[23] + ")";
    			    }
                }
            }
        }
        function StreamingClick()
        {
            var value = document.getElementById('DynamicSwitch').innerHTML;
            if(value == 'On')
            {
                document.getElementById('DynamicSwitch').innerHTML = "Off";
          	    setCookie('streamingdata','On',365);
          	    getQuoteData();
            }
            else
            {
               document.getElementById('DynamicSwitch').innerHTML = "On";
               setCookie('streamingdata','Off',365);
               clearTimeout ( timerid );
            }
        }
        if(document.getElementById('DynamicSwitch')!=null)
    	    getQuoteData();
        </script>
       <script type="text/javascript">
	    var tooltiptimer = "";
	    function showDelayedToolTip(eleID)
	    {
		    tooltiptimer = setTimeout("showToolTip('"+eleID+"');",1000);
	    }
	    function showToolTip(eleID)
	    {
		    document.getElementById(eleID).getElementsByTagName("span")[0].style.visibility = "visible";
		    tooltiptimer = "";		
	    }
	    function hideToolTip(eleID)
	    {		
		    if(tooltiptimer!="")
		    {
			    clearTimeout(tooltiptimer);
			    tooltiptimer = "";
		    }
		    document.getElementById(eleID).getElementsByTagName("span")[0].style.visibility = "hidden";
	    }
	</script>
