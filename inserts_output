    <script type="text/javascript">
var quoteBoxSelectedSymbol="BHAGCHEM.BO";

var quoteBoxSymbolArray = new Array(1);
quoteBoxSymbolArray[0] = "BHAGCHEM.BO";

var quoteBoxNumSymbols = 1;

